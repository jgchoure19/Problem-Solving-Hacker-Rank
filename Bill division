void bonAppetit(vector<int> bill, int annanot, int annaamo) {
      //int temp = bill[annanot];
      int add=0;
      int actual;
      for(int i=0;i<bill.size();i++){
        add+= bill[i];}
        
     actual=(add-bill[annanot])/2;
     if(actual==annaamo){
         cout<<"Bon Appetit";
     }
     else{
         int charged=annaamo-actual;
      cout<<charged;}
}
